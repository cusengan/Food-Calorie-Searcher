{
  "manifest_version": 2,

  "name": "Food Calorie Searcher",
  "description": "This extension will allow users to search a food and find its calorie count",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "https://trackapi.nutritionix.com/"
   ]
}